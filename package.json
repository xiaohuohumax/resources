{
  "name": "resources",
  "version": "4.10.0",
  "description": "资源仓库: 收录各种常用资源地址。",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "postinstall": "simple-git-hooks",
    "changeset": "changeset",
    "lint": "eslint .",
    "commit": "pnpm format && pnpm build && pnpm lint:fix && git add . && git-cz",
    "format": "vite-node scripts/format.ts",
    "linkinator": "vite-node scripts/linkinator.ts",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "resources",
    "vitepress"
  ],
  "author": {
    "name": "xiaohuohumax",
    "url": "https://github.com/xiaohuohumax"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaohuohumax/resources.git"
  },
  "license": "MIT",
  "simple-git-hooks": {
    "commit-msg": "pnpx commitlint --edit $1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "slugify": "^1.6.6",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@xiaohuohumax/bookmark": "^2.1.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.38.0",
    "globby": "^16.0.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "linkinator": "^7.5.0",
    "read-pkg": "^10.0.0",
    "simple-git-hooks": "^2.13.1",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite-node": "^5.2.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^2.0.0-alpha.15",
    "vitepress-plugin-rss": "^0.3.4"
  }
}
