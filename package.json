{
  "name": "resources",
  "version": "3.10.2",
  "description": "资源仓库: 收录各种常用资源地址。",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "check": "vite-node .vitepress/script/check.ts",
    "fmt-file": "vite-node .vitepress/script/fmt-file.ts",
    "check:cache": "npm run check -- --cache",
    "commit": "npm run lint:fix && npm run build && git add . && cz",
    "postinstall": "simple-git-hooks",
    "changeset": "changeset",
    "version": "changeset version && npm i",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "resources",
    "vitepress"
  ],
  "author": {
    "name": "xiaohuohumax",
    "url": "https://github.com/xiaohuohumax"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaohuohumax/resources.git"
  },
  "license": "MIT",
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@node-rs/jieba": "^1.10.4",
    "@types/micromatch": "^4.0.9",
    "@types/progress": "^2.0.7",
    "@xiaohuohumax/bookmark": "^2.1.2",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.9.1",
    "eslint": "^9.11.1",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "micromatch": "^4.0.7",
    "p-limit": "^6.1.0",
    "progress": "^2.0.3",
    "puppeteer": "^22.12.0",
    "simple-git-hooks": "^2.11.1",
    "vite-node": "^2.1.4",
    "vitepress": "^1.5.0"
  }
}
